			<pattern>META IDEB MUNICIPIO * PARA O ANO DE *</pattern>
			<template>►CSV IDEB META ANO◄ ; <star index = "1"/> ; <star index = "2"/></template>
		</category>

		<category>
			<pattern>META IDEB MUNICIPIO * PARA *</pattern>
			<template><srai>META IDEB MUNICIPIO <star index = "1"/> PARA O ANO DE <star index = "2"/></srai></template>
		</category>

		<category>
			<pattern>META IDEB MUNICIPIO * NO ANO DE *</pattern>
			<template><srai>META IDEB MUNICIPIO <star index = "1"/> PARA O ANO DE <star index = "2"/></srai></template>
		</category>

		<category>
			<pattern>META IDEB MUNICIPIO * EM *</pattern>
			<template><srai>META IDEB MUNICIPIO <star index = "1"/> PARA O ANO DE <star index = "2"/></srai></template>
		</category>

		<category>
			<pattern>META * IDEB * EM *</pattern>
			<template>►CSV IDEB META ANO◄ ; <star index = "2"/> ; <star index = "3"/></template>
		</category>

		<category>
			<pattern>META IDEB * EM *</pattern>
			<template><srai>META IDEB MUNICIPIO <star index = "1"/> PARA O ANO DE <star index = "2"/></srai></template>
		</category>


		<!-- Meta município geral -->
		<category>
			<pattern>* META * IDEB * MUNICIPIO *</pattern>
			<template>►CSV IDEB META◄ ; <star index = "4"/></template>
		</category>

		<category>
			<pattern>* META * IDEB *</pattern>
			<template>►CSV IDEB META◄ ; <star index = "3"/></template>
		</category>

		<category>
			<pattern>META IDEB MUNICIPIO *</pattern>
			<template>►CSV IDEB META◄ ; <star/></template>
		</category>

		<category>
			<pattern>META * IDEB *</pattern>
			<template>►CSV IDEB META◄ ; <star index = "2"/></template>
		</category>

		<category>
			<pattern>META IDEB *</pattern>
			<template><srai>META IDEB MUNICIPIO <star/></srai></template>
		</category>


		<!-- IDEB município por ano -->
		<category>
			<pattern>* IDEB DO MUNICIPIO * PARA O ANO DE *</pattern>
			<template>►CSV IDEB IDEB ANO◄ ; <star index = "2"/> ; <star index = "3"/></template>
		</category>

		<category>
			<pattern>* IDEB DO MUNICIPIO * PARA *</pattern>
			<template><srai><star index = "1"/> IDEB DO MUNICIPIO <star index = "2"/> PARA O ANO DE <star index = "3"/></srai></template>
		</category>

		<category>
			<pattern>* IDEB DO MUNICIPIO * NO ANO DE *</pattern>
			<template><srai><star index = "1"/> IDEB DO MUNICIPIO <star index = "2"/> PARA O ANO DE <star index = "3"/></srai></template>
		</category>

		<category>
			<pattern>* IDEB DO MUNICIPIO * EM *</pattern>
			<template><srai><star index = "1"/> IDEB DO MUNICIPIO <star index = "2"/> PARA O ANO DE <star index = "3"/></srai></template>
		</category>

		<category>
			<pattern>IDEB MUNICIPIO * PARA O ANO DE *</pattern>
			<template>►CSV IDEB IDEB ANO◄ ; <star index = "1"/> ; <star index = "2"/></template>
		</category>

		<category>
			<pattern>IDEB MUNICIPIO * PARA *</pattern>
			<template><srai>IDEB MUNICIPIO <star index = "1"/> PARA O ANO DE <star index = "2"/></srai></template>
