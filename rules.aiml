		<!-- Meta município por ano -->
		<category>
			<pattern>* META * IDEB * MUNICIPIO * PARA O ANO DE *</pattern>
			<template>►CSV IDEB META ANO◄ ; <star index = "4"/> ; <star index = "5"/></template>
		</category>

		<category>
			<pattern>* META * IDEB * MUNICIPIO * PARA *</pattern>
			<template><srai><star index = "1"/> META <star index = "2"/> IDEB <star index = "3"/> MUNICIPIO <star index = "4"/> PARA O ANO DE <star index = "5"/></srai></template>
		</category>

		<category>
			<pattern>* META * IDEB * MUNICIPIO * NO ANO DE *</pattern>
			<template><srai><star index = "1"/> META <star index = "2"/> IDEB <star index = "3"/> MUNICIPIO <star index = "4"/> PARA O ANO DE <star index = "5"/></srai></template>
		</category>

		<category>
			<pattern>* META * IDEB * MUNICIPIO * EM *</pattern>
			<template><srai><star index = "1"/> META <star index = "2"/> IDEB <star index = "3"/> MUNICIPIO <star index = "4"/> PARA O ANO DE <star index = "5"/></srai></template>
		</category>

		<category>
			<pattern>* META * IDEB * PARA O ANO DE *</pattern>
			<template>►CSV IDEB META ANO◄ ; <star index = "3"/> ; <star index = "4"/></template>
		</category>

		<category>
			<pattern>* META * IDEB * PARA *</pattern>
			<template><srai><star index = "1"/> META <star index = "2"/> IDEB <star index = "3"/> PARA O ANO DE <star index = "4"/></srai></template>
		</category>

		<category>
			<pattern>* META * IDEB * NO ANO DE *</pattern>
			<template><srai><star index = "1"/> META <star index = "2"/> IDEB <star index = "3"/> PARA O ANO DE <star index = "4"/></srai></template>
